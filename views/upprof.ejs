<body>
   
        
        <!-- CONTENT -->
        
     
    
        <div class="ar">  
    
      
           
       
            <br><br><br><br>
           
            <div class="container">
                <div class="arlo_tm_title_holder contact">
                    <h3>PROFIL</h3>
                    
                </div>
            </div>
           
    <form action="/upprof?id=<%=id%>&idf=<%=idf%>" method="post" class="contact_form" enctype="multipart/form-data" name="contact_form" id="contact_form">
         <div class="rightbox">
            <div class="arlo_tm_contact_wrap">
                <div class="main_input_wrap">
                   
                      
                        
                        <div class="wrap">
                            <input id="nom" name="nom" type="text" placeholder="votre nom" value="<%=pr.nom%>">
                        </div>
                        <div class="wrap">
                            <input id="prenom" name="prenom" type="text" placeholder="votre prenom"  value="<%=pr.prenom%>">
                        </div>
                        <div class="wrap">
                            <input id="email" name="email"  type="text" placeholder="votre Email" value="<%=pr.email%>">
                        </div>
                        <div class="wrap">
                            <input id="tel"  name="tel" type="text" placeholder="votre telephone"   value="<%=pr.tel%>">
                        </div>
                        <div class="wrap">
                            <input id="age" name="age"  type="number" placeholder="votre Age" value="<%=pr.age%>">
                        </div>
                        <div class="wrap">
                            <input id="adresse"  name="adresse" type="text" placeholder="votre Adresse" value="<%=pr.adresse%>">
                        </div>
                        <div class="wrap">
                            <input id="profession" name="profession" type="text" placeholder="votre Profession" value="<%=pr.profession%>">
                        </div>
                        <div class="wrap">
                            <input id="website" name="website" type="text" placeholder="votre Site web" value="<%=pr.website%>">
                        </div>

                        <div class="wrap">
                            <textarea id="message" name="message" placeholder="Parlez de vous "></textarea>
                        </div>
                        <div class="wrap">
                            <input type="file" name="photo"  id="photo" accept="image/png, image/jpeg"  >
                         
                            </div>
                           
                         </div>
                        <div class="suivant"><button type="submit">DONE</button></div>
                    
                </div>
            </div>
        </div>
        
        </div>
       
         
            </form>

            
            <div class="arlo_tm_footer_contact_wrapper_all">
                <div class="arlo_tm_contact_wrap_all">
                    
                    <div class="arlo_tm_footer_wrap">
                        <div class="container">
