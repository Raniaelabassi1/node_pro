<body>
   
        
        <!-- CONTENT -->
        
     
    
        <div class="ar">  
    
      
        <br><br><br><br>
           
       
           
           
                <div class="container">
                    <div class="arlo_tm_title_holder contact">
                        <h3>COMPETENCES</h3>
                        
                    </div>
                </div>
                 <form action="/upcomp?id=<%=id%>&idf=<%=idf%>" method="post" class="contact_form" name="contact_form" id="contact_form">
                <div class="rightbox">
                    <div class="arlo_tm_contact_wrap">
                        <div class="main_input_wrap">
                           
                              
                                
                                <div class="wrap">
                                    <input id="competence" name="Competence" type="text" placeholder="Competence" value="<%=comp.competence%>">
                                </div>
                                <br><br><br>
                                <div class="wrap">
                                   
                                    <label for="debutant">Debutant</label>
                                    <input type="checkbox" id="debutant" name="niveau" value="debutant">
                                    <label for="intermediaire">Intermediaire</label>
                                    <input type="checkbox" id="intermediaire" name="niveau" value="intermediaire">
                                    <label for="niveau avance">Niveau avance</label>
                                    <input type="checkbox" id="niveau avance" name="niveau" value="niveau avance">
                                    <label for="expert">Expert</label>
                                    <input type="checkbox" id="expert" name="niveau" value="expert">
                                    
                                </div>
                                <div class="wrap">
                                    <input id="ans" name="ans" type="number" placeholder="Année d'expériences" value="<%=comp.ans%>">
                                </div>
                               
                                
                                <button type="submit" class="competence" >DONE/ ADD</button>
                               
                               
                           
                        </div>
                    </div>
                </div>
                
            
        
            </form>
            <div class="arlo_tm_footer_contact_wrapper_all">
                <div class="arlo_tm_contact_wrap_all">
                    
                    <div class="arlo_tm_footer_wrap">
                        <div class="container">
                            <p>&copy; Copyright 2020. All Rights are Reserved.</p>
                        </div>
                    </div>
                </div>
            </div>
           
